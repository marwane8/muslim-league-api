import abc



class Big(abc.ABC):
    def __init__(self):
        self.db_lookup = {"DB" : 1 }
    
    def play(self):
        print("im playying!!", self.db_lookup["DB"])
    
    @abc.abstractclassmethod
    def run():
        print("run for itQ")

class Small(Big):
    def run(self):
        print("in running")
    def go(self):
        print("im going!!", self.db_lookup["DB"])


s = Small()
s.play()
s.go()

b = Big()
b.play()

